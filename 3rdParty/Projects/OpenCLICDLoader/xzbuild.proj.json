{
    "name": "OpenCLICDLoader",
    "type": "dynamic",
    "description": "Khronos official OpenCL ICD Loader",
    "version": "3.0.5",
    "dependency": [],
    "srcPath": "../../OpenCL-ICD-Loader/loader",
    "library": 
    {
        "static": [],
        "dynamic": ["dl", "pthread"]
    },
    "expmap": "linux/icd_exports.map",
    "targets":
    {
        "cxx":
        {
            "defines": ["OpenCL_EXPORTS", "CL_TARGET_OPENCL_VERSION=300", "OPENCL_ICD_LOADER_VERSION_MAJOR=3", "OPENCL_ICD_LOADER_VERSION_MINOR=0", "OPENCL_ICD_LOADER_VERSION_REV=5", "CL_ENABLE_LAYERS"],
            "visibility": "default"
        },
        "c":
        {
            "sources": ["*.c", "linux/*.c", {"-": "cllayerinfo.c"}],
            "incpath": ["$(BuildDir)", "."],
            "version": ""
        },
        "rc":
        {
            "sources": ["*.rc"]
        }
    }
}