{
    "name": "zlib",
    "type": "static",
    "description": "A massively spiffy yet delicately unobtrusive compression library",
    "version": "1.2.11",
    "dependency": [],
    "library": 
    {
        "static": [],
        "dynamic": []
    },
    "targets":
    {
        "c":
        {
            "sources": ["*.c"],
            "defines": 
            [
                "_LARGEFILE64_SOURCE=1", "NO_UNDERLINE", "ASMV",
                "HAVE_HIDDEN", "HAVE_UNISTD_H", "HAVE_STDARG_H"
            ]
        },
        "asm":
        {
            "sources": 
            [
                {"ifeq": {"platform": "x64"}, "+": "masmx64/*.S"},
                {"ifeq": {"platform": "x86"}, "+": "masmx86/*.S"}
            ]
        }
    }
}