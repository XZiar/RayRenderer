{
    "name": "zlib-ng",
    "type": "static",
    "description": "zlib replacement with optimizations for \"next generation\" systems",
    "version": "1.9.9",
    "dependency": [],
    "library": 
    {
        "static": [],
        "dynamic": []
    },
    "targets":
    {
        "c":
        {
            "defines": 
            [
                {"ifhas": "pclmul", "+": ["X86_PCLMULQDQ_CRC"]},
                {"ifhas": "sse42", "+": ["X86_SSE4_2_CRC_HASH", "X86_SSE4_2_CRC_INTRIN"]},
                "_LARGEFILE64_SOURCE=1", "WITH_GZFILEOP",
                "HAVE_HIDDEN", "HAVE_INTERNAL", "HAVE_BUILTIN_CTZL",
                "HAVE_UNISTD_H", "HAVE_STDARG_H",
                "ZLIB_COMPAT", "MEDIUM_STRATEGY", "UNROLL_LESS", "UNALIGNED_OK",
                "X86_NOCHECK_SSE2", "X86_CPUID", "X86_SSE2_FILL_WINDOW", "X86_QUICK_STRATEGY"
            ],
            "sources": ["*.c", "arch/i386/*.c", {"-": ["arch/i386/crc_folding.c"]}],
            "incpath": ["."]
        }
    }
}