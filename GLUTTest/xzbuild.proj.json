{
    "name": "GLUTTest",
    "type": "executable",
    "description": "Test Program based on FreeGLUT",
    "dependency": ["FreeGLUTView", "OpenGLUtil", "RenderCore", "FontHelper", "ResourcePackager", "ImageUtil", "StringUtil"],
    "library": 
    {
        "static": 
        [
            {"ifhas": "fslib_s", "+": "@fslib_s"}
        ],
        "dynamic": 
        [
            "X11",
            {"ifhas": "fslib_d", "+": "@fslib_d"}
        ]
    },
    "targets":
    {
        "cpp":
        {
            "sources": ["*.cpp"],
            "flags": ["-Wno-unused-function", "-Wno-switch"]
        }
    }
}